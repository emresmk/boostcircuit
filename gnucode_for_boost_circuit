VS = 23; # V  DC Kaynak gerilimi Volt ile ifade ediliyor.
D = 0.73; # s  Görev süresi (duty cycle) saniye ile ifade ediliyor.
L = 149d-6;# H   Bobin Henry ile ifade ediliyor.
C = 81d-6;#  F  Kapasitör değeri Farad ile ifade ediliyor.
f = 20000; # Hz  Anahtarlama frekansı Hz ile ifade ediliyor.
T = 1/f; # s Bir dalganın tamamlama süresi saniye ile ifade ediliyor. Periyot.
R = 103; # Ohm Çıkış direnç değeri ohm ile ifade ediliyor.
VO = VS*(1/(1-D)); # Çıkış gerilim ifadesi
gerilim= VS - VO; # Bobinin üzerine düşen gerilim

F1 = @(zaman1,akim1)(VS/L); #Anahtar kapalı durumununda bobin akımı- zaman fonksiyonu
F2 = @(zaman2,akim2)((VS-VO)/L); # Anahtar açık durumunda bobin akımı- zaman fonksiyonu
IL = VS*(1/(((1-D)^2)*R));
disp(IL);
dil = (VS-VO)*(1/L)*(1-D)*T; # Akımın değişimi
akim1min = IL + (0.5*dil); # A Akımın minimum değeri amper ile ifade ediliyor. ( Akımın başlangıç değeridir )
[zaman1,akim1] = ode45(F1,[0 D*T],akim1min); # Anahtar kapalı olduğu durumda, zaman ve akim değerleri
akim1max = akim1(length(akim1)); # A Akımın makismum değeri amper ile ifade ediliyor. ( Anahtarın pozisyon değiştirdiği an )

[zaman2,akim2] = ode45(F2,[D*T T],akim1max); # Anahtar açık olduğu durumda, zaman ve akım değerleri
plot(zaman1,akim1,zaman2,akim2); # Anahtar kapalı ve açık durumunda akım-zaman grafiği
xlabel ("Zaman t(s)");
ylabel ("Akım I(A)");
title ("Boost devresi için akım-zaman değişiminde mavi eğri anahtar kapalı durumu, turuncu eğri anahtar açık durumunda değişim");

#BOOST devresinin çıkış akımının değişim çalışması

